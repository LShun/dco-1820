\begin_inset Formula $\vartheta=0$
\end_inset

 and 
\begin_inset Formula $\vartheta=\frac{\pi}{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{dx}{d\vartheta} & =2\cdot3\left(\cos\vartheta\right)^{2}\left(-\sin\vartheta\right)\\
\frac{dx}{d\vartheta} & =-6\cos^{2}\vartheta\sin\vartheta
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{dy}{d\vartheta} & =2\cdot3\left(\sin\vartheta\right)^{2}\left(\cos\vartheta\right)\\
 & =6\sin^{2}\vartheta\cos\vartheta
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
L & =\int_{0}^{\frac{\pi}{2}}\sqrt{\left(-6\cos^{2}\vartheta\sin\vartheta\right)^{2}+\left(6\sin^{2}\vartheta\cos\vartheta\right)^{2}}d\vartheta\\
 & =\int_{0}^{\frac{\pi}{2}}\sqrt{36\cos^{4}\vartheta\sin^{2}\vartheta+36\sin^{4}\vartheta\cos^{2}\vartheta}d\vartheta\\
 & =\int_{0}^{\frac{\pi}{2}}\sqrt{36\cos^{2}\vartheta\sin^{2}\vartheta\left(\cos^{2}\vartheta+\sin^{2}\vartheta\right)}d\vartheta\\
 & =\int_{0}^{\frac{\pi}{2}}6\cos\vartheta\sin\vartheta\sqrt{1}d\vartheta\quad\text{Note: }\sin^{2}\theta+\cos^{2}\theta=1\\
 & =\int_{0}^{\frac{\pi}{2}}3\left(2\cos\vartheta\sin\vartheta\right)d\vartheta\\
 & =3\int_{0}^{\frac{\pi}{2}}\sin\left(2\vartheta\right)d\vartheta\quad\text{Note:}\text{2\ensuremath{\cos\theta\sin\theta=1}}\\
 & =3\left[-\frac{1}{2}\cos\left(2\vartheta\right)\right]_{0}^{\frac{\pi}{2}}\\
 & =3\cdot1\\
 & =3
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Alternatively, substitution rule:
\begin_inset Formula 
\begin{align*}
L & =6\int_{0}^{\frac{\pi}{2}}\cos\vartheta\sin\vartheta d\vartheta
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $u=\sin\vartheta$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{du}{d\vartheta} & =\cos\vartheta\\
du & =\cos\vartheta d\vartheta
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
u|_{\vartheta=\frac{\pi}{2}}=1,u|_{\vartheta=0}=0
\]

\end_inset


\begin_inset Formula 
\begin{align*}
L & =6\int_{0}^{1}udu\\
 & =6\cdot\frac{1}{2}\\
L & =3
\end{align*}

\end_inset


\end_layout

