\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $p\lor q\lor\left(\sim p\land\sim q\land r\right)\equiv p\lor q\lor r$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
LHS (Using a lot of OR distributive law)
\begin_inset Formula 
\begin{align*}
p+q+\bar{p}\bar{q}r & \equiv p+\bar{p}\bar{q}r+q\\
 & \equiv p+\bar{p}\left(\bar{q}r\right)+q\\
 & \equiv\left(p+\bar{p}\right)\left(p+\bar{q}r\right)+q\\
 & \equiv1\left(p+\bar{q}r\right)+q\\
 & \equiv p+\left(q+\bar{q}r\right)\\
 & \equiv p+\left(\left(q+\bar{q}\right)\cdot\left(q+r\right)\right)\\
 & \equiv p+\left(q+r\right)\\
 & \equiv p+q+r\\
 & \equiv RHS
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\sim\left(p\downarrow q\right)\equiv\left(\sim p\vert\sim q\right)$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
LHS
\begin_inset Formula 
\begin{align*}
\sim\left(p\downarrow q\right) & \equiv\sim\left(\sim\left(p\lor q\right)\right)\\
 & \equiv p\lor q
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
RHS
\begin_inset Formula 
\begin{align*}
\left(\sim p\vert\sim q\right) & \equiv\sim\left(\sim p\land\sim q\right)\\
 & \equiv\sim\left(\sim p\land\sim q\right)\\
 & \equiv\left(p\lor q\right)\,\text{(DeMorgan's)}\\
 & \equiv LHS
\end{align*}

\end_inset


\end_layout

