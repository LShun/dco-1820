\begin_inset Formula 
\begin{align*}
\left(p\lor q\right)\land\sim\left(\sim p\land q\right) & \equiv\left(p\lor q\right)\land\left(p\lor\sim q\right)\\
 & \equiv\left(p+q\right)\left(p+\bar{q}\right)\\
 & \equiv pp+pq+pq+p\bar{q}\\
 & \equiv p+pq+p\bar{q}\\
 & \equiv p\left(1+q+\bar{q}\right)\\
 & \equiv p\left(1\right)\\
 & \equiv p
\end{align*}

\end_inset


