\begin{align*}
\left(\left(p\lor q\right)\land\left(p\lor\sim q\right)\right)\lor q & \equiv\left(\left(p+q\right)\left(p+\bar{q}\right)\right)+q\\
 & \equiv\left(pp+pq+p\bar{q}+q\bar{q}\right)+q\\
 & \equiv\left(p\left(1+q+\bar{q}\right)+1\right)+q\\
 & \equiv\left(p\left(1\right)\right)+q\\
 & \equiv p+q\\
 & \equiv p\lor q
\end{align*}

\end_inset


\end_layout

\end_deeper
