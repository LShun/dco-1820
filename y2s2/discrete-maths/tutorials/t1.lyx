\end_inset


\begin_inset Formula 
\begin{align*}
\sim T\land\left(F\lor T\right) & \equiv\sim T\land T\\
 & \equiv F
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $p\land\left(\sim\left(q\lor\sim r\right)\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
p\land\left(\sim\left(q\lor\sim r\right)\right) & \equiv T\land\left(\sim\left(F\lor\sim T\right)\right)\\
 & \equiv T\land\left(\sim F\right)\\
 & \equiv T\land T\\
 & \equiv T
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(r\land\sim q\right)\lor\left(p\land r\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\left(r\land\sim q\right)\lor\left(p\land r\right) & \equiv\left(T\land\sim F\right)\lor\left(T\land T\right)\\
 & \equiv\left(T\land T\right)\lor T\\
 & \equiv T\lor T\\
 & \equiv T
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\left(q\land r\right)\land\left(p\land\sim r\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\left(q\land r\right)\land\left(p\land\sim r\right) & \equiv\left(F\land T\right)\land\left(T\land\sim T\right)\\
 & \equiv F\land\left(T\land F\right)\\
 & \equiv F\land F\\
 & \equiv F
\end{align*}

\end_inset


\end_layout

