\begin_layout Enumerate
\begin_inset Formula $\int_{4}^{6}\frac{12}{\left(x-3\right)\left(x+1\right)}dx$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Partial fractions!
\begin_inset Formula 
\begin{align*}
\frac{12}{\left(x-3\right)\left(x+1\right)} & =\frac{A}{\left(x-3\right)}+\frac{B}{\left(x+1\right)}\\
12 & =A\left(x+1\right)+B\left(x-3\right)\\
A\left(x+1\right)+B\left(x-3\right)-12 & =0
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Find 
\begin_inset Formula $A$
\end_inset

, when 
\begin_inset Formula $x=3$
\end_inset


\begin_inset Formula 
\begin{align*}
A\left(3+1\right)+B\left(3-3\right)-12 & =0\\
4A-12 & =0\\
A & =\frac{12}{4}\\
 & =3
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find 
\begin_inset Formula $B$
\end_inset

, when 
\begin_inset Formula $x=-1$
\end_inset


\begin_inset Formula 
\begin{align*}
A\left(-1+1\right)+B\left(-1-3\right)-12 & =0\\
-4B-12 & =0\\
B & =-\frac{12}{4}\\
 & =-3
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find the fractions
\begin_inset Formula 
\begin{align*}
\frac{12}{\left(x-3\right)\left(x+1\right)} & =\frac{A}{\left(x-3\right)}+\frac{B}{\left(x+1\right)}\\
 & =\frac{3}{x-3}-\frac{3}{x+1}
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
Find the integration
\begin_inset Formula 
\begin{align*}
\int_{4}^{6}\frac{12}{\left(x-3\right)\left(x+1\right)}dx & =\int_{4}^{6}\left(\frac{3}{x-3}-\frac{3}{x+1}\right)dx\\
 & =3\int_{4}^{6}\left(\frac{1}{x-3}-\frac{1}{x+1}\right)dx\\
 & =3\left[\int_{4}^{6}\frac{1}{x-3}dx-\int_{4}^{6}\frac{1}{x+1}dx\right]\\
 & =3\left[\left[\ln\left(x-3\right)\right]_{4}^{6}-\left[\ln\left(x+1\right)\right]_{4}^{6}\right]\\
 & =3\left[\ln3-\ln1-\left(\ln7-\ln5\right)\right]\\
 & =3\left[\ln3-\ln1-\ln7+\ln5\right]\\
 & =3\left[\ln\frac{3*5}{7}\right]\\
 & =3\ln\frac{15}{7}
\end{align*}

\end_inset


\end_layout

