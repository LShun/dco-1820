\begin_inset Formula 
\begin{align*}
\int_{0}^{3}\sqrt{9-x^{2}}dx & =\int_{0}^{3}\sqrt{9-\left(3\cos\theta\right)^{2}}dx\\
 & =\int_{0}^{3}\sqrt{9-9\cos^{2}\theta}dx\\
 & =\int_{0}^{3}\sqrt{9\left(1-\cos^{2}\theta\right)}dx\\
 & =\int_{0}^{3}3\sqrt{1-\cos^{2}\theta}dx\\
 & =3\int_{0}^{3}\sqrt{1-\left(1-\sin^{2}\theta\right)}dx\\
 & =3\int_{0}^{3}\sqrt{1-\left(1-\sin^{2}\theta\right)}dx\\
 & =3\int_{0}^{3}\sqrt{\sin^{2}\theta}dx\\
 & =3\int_{0}^{3}\sin\theta dx\\
 & =3\int_{\frac{\pi}{2}}^{0}\sin\theta\left(-3\sin\theta d\theta\right)\\
 & =-9\int_{\frac{\pi}{2}}^{0}\sin^{2}\theta d\theta\\
 & =-9\int_{\frac{\pi}{2}}^{0}\frac{1+\cos\left(2\theta\right)}{2}\\
 & =-\frac{9}{2}\int_{\frac{\pi}{2}}^{0}1+\cos\left(2\theta\right)\\
 & =-\frac{9}{2}\left[\theta-2\sin\left(2\theta\right)\right]_{\frac{\pi}{2}}^{0}\\
 & =\frac{9}{2}\left(\frac{\pi}{2}\right)\\
 & =\frac{9}{4}\pi
\end{align*}

