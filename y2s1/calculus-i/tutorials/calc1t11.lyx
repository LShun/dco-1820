\begin_layout Enumerate
If 
\begin_inset Formula $f\left(x\right)=\ln x-1$
\end_inset

, 
\begin_inset Formula $1\le x\le4$
\end_inset

 , evaluate the Riemann sum with 
\begin_inset Formula $n=6$
\end_inset

, taking the sample points to be left endpoints.
 Give your answer correct to six decimal places
\begin_inset Formula 
\[
n=6,\Delta x=\frac{4-1}{6}=0.5
\]

\end_inset


\begin_inset Formula 
\begin{align*}
R_{6} & \approx0.5\left[\left(\ln1-1\right)+\left(\ln1.5-1\right)+\left(\ln2-1\right)+\left(\ln2.5-1\right)+\left(\ln3-1\right)+\left(\ln3.5-1\right)\right]\\
 & =-0.816861
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find an approximation to the integral 
\begin_inset Formula $\int_{0}^{4}\left(x^{2}-3x\right)dx$
\end_inset

 using a Riemann sum with right endpoints and 
\begin_inset Formula $n=8$
\end_inset

.
\begin_inset Formula 
\[
\int_{0}^{4}\left(x^{2}-3x\right)dx,n=8,\Delta x=\frac{4-0}{8}=0.5
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\int_{0}^{4}\left(x^{2}-3x\right)dx & \approx0.5\left(\left(\left(0.5\right)^{2}-3\left(0.5\right)\right)+\left(\left(1\right)^{2}-3\left(1\right)\right)+\left(\left(1.5\right)^{2}-3\left(1.5\right)\right)+\left(\left(2\right)^{2}-3\left(2\right)\right)+\left(\left(2.5\right)^{2}-3\left(2.5\right)\right)+\left(\left(3\right)^{2}-3\left(3\right)\right)+\left(\left(3.5\right)^{2}-3\left(3.5\right)\right)+\left(\left(4\right)^{2}-3\left(4\right)\right)\right)\\
 & =\text{−}1.5
\end{align*}

\end_inset


\end_layout

